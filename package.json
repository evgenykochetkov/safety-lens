{
  "name": "safety-lens",
  "version": "1.5.0",
  "description": "Type-safe, functional lens library supporting Immutable.js",
  "author": "Jesse Hallett <jesse@sitr.us>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/hallettj/safety-lens",
  "repository": {
    "type": "git",
    "url": "https://github.com/hallettj/safety-lens.git"
  },
  "main": "lens.js",
  "scripts": {
    "pretest": "make",
    "test": "babel-node node_modules/.bin/_mocha --full-trace --recursive -R spec -C --check-leaks test/",
    "typecheck": "flow check",
    "prepublishOnly": "npm run typecheck && make && npm test",
    "prepublish": "(not-in-publish && echo 'Skipping prepublish') || npm run require-npm4-to-publish",
    "require-npm4-to-publish": "semver -r '>=4.0.0' $(npm --version) || (echo 'NPM 4+ required to publish' && exit 1)"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "keywords": [
    "lens",
    "lenses",
    "functional",
    "Flow",
    "cursors",
    "immutable",
    "Immutable.js"
  ],
  "files": [
    "lens.js",
    "lens.js.flow",
    "lens.js.map",
    "immutable.js",
    "immutable.js.flow",
    "immutable.js.map",
    "es2015.js",
    "es2015.js.flow",
    "es2015.js.map",
    "lib"
  ],
  "directories": {
    "doc": "doc"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "chai-immutable": "^1.6.0",
    "deep-equal": "^1.0.1",
    "flow-bin": "^0.48.0",
    "immutable": "^3.8.1",
    "in-publish": "^2.0.0",
    "jsverify": "^0.8.2",
    "mocha": "^3.4.2",
    "mori": "^0.3.2",
    "prettier-standard": "^5.1.0"
  },
  "peerDependencies": {}
}
